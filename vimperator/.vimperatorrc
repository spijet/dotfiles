highlight Bell border: none; background: #222326;
highlight Boolean color: #aaabad;
highlight CmdLine color: #EBECEE; background: #222326;
highlight CmdLine>* font-family: "Iosevka"; font-size: 10pt; height: 24px;
highlight CompDesc width: auto; color: #AAABAD;
highlight CompItem background: #222326;
highlight CompItem>* height: auto; padding: 0 8px;
highlight CompItem[selected] color: #EBECEE; background: #7955da;
highlight CompTitle background: #222326; font-family: "Iosevka"; font-size: 10pt;
highlight CompTitle>* color: #ebecee; padding: 2px 8px;
highlight ContentSeparator display: -moz-box;
highlight Disabled color: #aaabad;
highlight Enabled color: #AAABAD;
highlight ErrorMsg color: #F56C66; font-family: "Iosevka"; font-size: 10pt;
highlight Filter color: #ebecee; background: #6c4cc0;
highlight FrameIndicator background: #AAABAD; opacity: 0.5; z-index: 999; position: fixed; top: 0; bottom: 0; left: 0; right: 0;
highlight HelpNote color: #8859B3;
highlight HelpWarning color: #B359A4;
highlight Hint font-family: "Iosevka"; font-size: 10pt; color: #ebecee; background: #7955da; padding: 1px;
highlight HintActive color: #AAABAD; background: #222326;
highlight HintElem background: transparent;
highlight HintImage opacity: 0.5;
highlight InfoMsg color: #AAABAD;
highlight Keyword color: #AAABAD;
highlight LineNr color: #aaabad;
highlight ModeMsg color: #AAABAD; background: transparent; padding: 0 8px; font-family: "Iosevka"; font-size: 10pt;
highlight MoreMsg color: #aaabad; font-family: "Iosevka"; font-size: 10pt;
highlight NonText color: #aaabad;
highlight Normal color: #AAABAD; background: #222326; font-family: "Iosevka"; font-size: 10pt;
highlight PromptText color: #AAABAD; padding-top: 4px;
highlight Question color: #AAABAD;
highlight StatusLine color: #AAABAD; background: #222326; font-family: "Iosevka"; font-size: 10pt;
highlight Title color: #ebecee;
highlight WarningMsg color: #AAABAD;

map J gt
map K gT

set hintchars=hjklasdfgyuiopqwertnmzxcvb
set "titlestring=Mozilla Firefox"
set toolbars=noaddons,nobookmarks,nomenu,nonavigation,tabs

js services.services.autoCompleteSearch.class_="@mozilla.org/autocomplete/search;1?name=unifiedcomplete"

